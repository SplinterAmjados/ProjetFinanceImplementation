{{ form_start(form, { 'method' : 'POST' , 'action' : path('new_client_2')  }  ) }}
<table  >
<tr><td width="50%">NCIN/N°Passeport<span class="form-required">*</span></td><td>{{ form_widget(form.ncin, {'attr' : { 'placeholder' : 'NCIN' } }) }}<br>
{{ form_widget(form.nPasseport , {'attr' : { 'placeholder' : 'N° Passeport' } }) }}		</td></tr>
<tr><td>Nom<span class="form-required">*</span></td><td>{{ form_widget(form.nom) }}</td></tr>
<tr><td>Prénom<span class="form-required">*</span></td><td>{{ form_widget(form.prenom) }}</td></tr>
<tr><td>Adresse<span class="form-required">*</span></td><td>{{ form_widget(form.adresse) }}</td></tr>
<tr><td>Ville<span class="form-required">*</span></td><td>{{ form_widget(form.ville,{'attr' : { 'class' : 'villes'} }) }}</td></tr>
<tr><td>Code Postal<span class="form-required">*</span></td><td>{{ form_widget(form.codePostal) }}</td></tr>
<tr><td>Tél<span class="form-required">*</span></td><td>{{ form_widget(form.tel) }}</td></tr>
</table>
<input type="hidden" name="natureClient" value="physique">
<input type="submit" value="Suivant">
{{ form_end(form) }}